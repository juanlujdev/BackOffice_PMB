n        }\r\n    }\r\n\r\n    bets = () => {\r\n        axios.get('https://localhost:44301/api/apuestas').then((resultRequest) => {\r\n            this.setState({userInfoData: resultRequest.data});\r\n        })\r\n    };\r\n\r\n    markets = () => {\r\n        axios.get('https://localhost:44301/api/mercados').then((resultRequest) => {\r\n            resultRequest.data.map((item) => {\r\n                if (item.Bloqueado === true) {\r\n                    item.Bloqueado = 'si';\r\n                } else {\r\n                    item.Bloqueado = 'no';\r\n                }\r\n            })\r\n            this.setState({marketInfoData: resultRequest.data});\r\n        })\r\n    };\r\n    blockMarket = () => {\r\n\r\n        axios.put('https://localhost:44301/api/mercados?id=' + this.state.saveInputId).then((resultRequest) => {\r\n            alert(\"El mercado con Id \"+this.state.saveInputId+\" esta bloqueado\");\r\n            this.setState({hideTableMarket: resultRequest.data, saveInputId: ''});\r\n            this.markets();\r\n\r\n        }).catch(function (error){\r\n            alert(\"Error\"+error.message.toString());\r\n        })\r\n    };\r\n}\r\n","C:\\Users\\juani\\2_DAM\\PMDM\\Ejer_evaluables\\pmb-backoffice\\src\\EventsView.js",[],"C:\\Users\\juani\\2_DAM\\PMDM\\Ejer_evaluables\\pmb-backoffice\\src\\UsersView.js",["35"],{"ruleId":"36","replacedBy":"37"},{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","severity":1,"message":"41","line":175,"column":43,"nodeType":"42","messageId":"43","endLine":175,"endColumn":45},{"ruleId":"44","severity":1,"message":"45","line":82,"column":52,"nodeType":"46","endLine":82,"endColumn":88},"no-native-reassign",["47"],"no-negated-in-lhs",["48"],"array-callback-return","Array.prototype.map() expects a return value from arrow function.","ArrowFunctionExpression","expectedInside","react/jsx-no-duplicate-props","No duplicate props allowed","JSXAttribute","no-global-assign","no-unsafe-negation"]